{
  "version": 1,
  "include": ["/*"],
  "exclude": [
    "/api/*",
    "/_nuxt/*",
    "/__nuxt_*",
    "/__og-image__/*",
    "/_fonts/*",
    "/_i18n/*",
    "/r/*",

    "/docs/api/*",
    "/docs/_nuxt/*",
    "/docs/__nuxt_*",
    "/docs/__og-image__/*",
    "/docs/_fonts/*",
    "/docs/_i18n/*",
    "/docs/r/*",

    "/*.png",
    "/*.jpg",
    "/*.jpeg",
    "/*.svg",
    "/*.webp",
    "/*.avif",
    "/*.gif",
    "/*.ico",
    "/*.woff",
    "/*.woff2",
    "/*.ttf",
    "/*.otf",
    "/*.css",
    "/*.js",
    "/*.map",

    "/docs/*.png",
    "/docs/*.jpg",
    "/docs/*.jpeg",
    "/docs/*.svg",
    "/docs/*.webp",
    "/docs/*.avif",
    "/docs/*.gif",
    "/docs/*.ico",
    "/docs/*.woff",
    "/docs/*.woff2",
    "/docs/*.ttf",
    "/docs/*.otf",
    "/docs/*.css",
    "/docs/*.js",
    "/docs/*.map"
  ]
